     evento.`id_tarea` AS evento_id_tarea,
     evento.`id_trabajador` AS evento_id_trabajador,
     tareas.`id` AS tareas_id,
     tareas.`nombre` AS tareas_nombre
FROM
     `tareas` tareas INNER JOIN `evento` evento ON tareas.`id` = evento.`id_tarea`
     INNER JOIN `trabajadores` trabajadores ON evento.`id_trabajador` = trabajadores.`id`]]>
