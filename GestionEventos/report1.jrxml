	<queryString>
		<![CDATA[SELECT
     trabajadores.`id` AS trabajadores_id,
     trabajadores.`nombre` AS trabajadores_nombre,
     trabajadores.`correo` AS trabajadores_correo,
     trabajadores.`id_tarea` AS trabajadores_id_tarea,
     evento.`id` AS evento_id,
